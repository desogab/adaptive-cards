{
  "branchName": "@{replace(triggerBody()?['ref'], 'refs/heads/', '')}",
  "commitCountText": "@{if(equals(triggerBody()?['total_commits_count'], 1), '1 commit', concat(string(triggerBody()?['total_commits_count']), ' commits'))}",
